{
    "ConnectionStrings": {
        "template": "Server=${DB_HOST};User Name=${DB_USER_NAME};Password=${DB_USER_PASSWORD};Database=${DB_NAME}"
    },
    "Params": {
        "DefaultConnectionString": "template",
        "RestApiUrl": "${REST_API_URL}",
        "WebSocketPath": "/elite-crew"
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Trace"
        }
    },
    "ApplicationInsights": {
        "LogLevel": {
            "Default": "None"
        }
    }
}